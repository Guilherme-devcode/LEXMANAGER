{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=../../packages/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=../../packages/database/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=../../packages/database/prisma/schema.prisma",
    "postinstall": "prisma generate --schema=../../packages/database/prisma/schema.prisma"
  },
  "prisma": {
    "schema": "../../packages/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.2",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@prisma/client": "^5.9.1",
    "@lexmanager/database": "workspace:*",
    "@lexmanager/shared": "workspace:*",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.4.2",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.9",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "prisma": "^5.9.1",
    "@nestjs/testing": "^10.3.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.11.5",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
